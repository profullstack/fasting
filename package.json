{
  "name": "@profullstack/fasting",
  "version": "1.3.0",
  "description": "A comprehensive CLI and Node.js module for 16:8 intermittent fasting with meal tracking, weight monitoring, and fast history with visual charts",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin",
    "examples",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/profullstack/fasting.git"
  },
  "bugs": {
    "url": "https://github.com/profullstack/fasting/issues"
  },
  "homepage": "https://profullstack.com",
  "bin": {
    "fasting": "./bin/cli.js"
  },
  "scripts": {
    "test": "node tests/run-tests.js",
    "test:fast": "node --test tests/fast.test.js",
    "test:charts": "node --test tests/charts.test.js",
    "test:fasting": "node --test tests/fasting.test.js",
    "test:weight": "node --test tests/weight.test.js",
    "test:calorie-chart": "node --test tests/calorie-chart.test.js",
    "test:exercise": "node --test tests/exercise.test.js",
